{
  "version": 2,
  "builds": [
    { "src": "UniStartupBoard.html", "use": "@vercel/static" },
    { "src": "data/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/UniStartupBoard.html" },
    { "src": "/data/(.*)", "dest": "/data/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
