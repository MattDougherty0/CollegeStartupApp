{
  "version": 2,
  "builds": [
    {
      "src": "CollegeApp/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "CollegeApp/data/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/data/(.*)",
      "dest": "/CollegeApp/data/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/CollegeApp/UniStartupBoard.html"
    }
  ]
}
